<h2>Comerce</h2>

     <table>
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Stock</th>
            </tr>

        </thead>
        <tbody>
            {{#each docs}}
                <tr>
                    <td>{{this.nombre}}</td>
                    <td>{{this.precio}}</td>
                    <td>{{this.stock}}</td>
                    <td>{{this.limit}}</td>

                    </tr>
            {{/each}}

        </tbody>
     </table>

     <div>
        {{#if hasPrevPage }}
            <a href="{{prevLink}}">Anterior</a>
        {{/if}}
        {{#if hasNextPage}}
            <a href="{{nextLink}}">Siguiente</a>  
        {{/if}}      
     </div>

     <div>
        <div>TotalPages:{{totalDocs}}</div>
        <div>PrevPage:{{prevPage}}</div>
        <div>NextPage:{{nextPage}}</div>
        <div>Page:{{page}}</div>
        <div>hasPrevPage:{{hasPrevPage}}</div>
        <div>hasNextPage:{{hasNextPage}}</div>
        <div>PrevLink :{{prevLink}} </div>
        <div>NextLink :{{nextLink}} </div>
     </div>